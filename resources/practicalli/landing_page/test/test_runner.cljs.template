;; Command Line test runner
;; - require `-test` namespace to include its tests in the runner
;; ---------------------------------------------------------


(ns {{top/ns}}.test-runner
  (:require
   [{{top/ns}}.{{main/ns}}-test]
   [figwheel.main.testing :refer [run-tests-async]]))

(defn -main [& args]
  (run-tests-async 5000))
